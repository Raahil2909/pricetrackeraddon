{
    "name":"Price Tracker Addon",
    "description":"Track amazon item prices",
    "version":"0.0",
    "manifest_version": 3,
    "background":{
        "service_worker":"background.js"
    },

    "action":{
        "default_popup":"popup.html"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["content.js"]
        }
    ],
    "permissions":["tabs","activeTab","storage"]
}